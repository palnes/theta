{
  "cmp": {
    "switch": {
      "width": {
        "$value": "{sys.dimension.component.spacing.xl}"
      },
      "height": {
        "$value": "{sys.dimension.component.spacing.md}"
      },
      "thumbSize": {
        "$value": "{sys.dimension.component.spacing.sm}"
      },
      "borderRadius": {
        "$value": "{sys.radius.full}"
      },
      "padding": {
        "$value": "{sys.spacing.xs}"
      },
      "transitionDuration": {
        "$value": "{sys.duration.normal}"
      },
      "color": {
        "track": {
          "default": {
            "$value": "{sys.color.surface.tertiary.default}"
          },
          "hover": {
            "$value": "{sys.color.interaction.brand.subtle.hover}"
          },
          "active": {
            "$value": "{sys.color.action.primary.default}"
          },
          "disabled": {
            "$value": "{sys.color.surface.disabled.default}"
          }
        },
        "thumb": {
          "default": {
            "$value": "{sys.color.surface.primary.inverse}"
          },
          "disabled": {
            "$value": "{sys.color.surface.secondary.default}"
          }
        }
      }
    }
  }
}
